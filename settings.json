{
  /*
  * Use ":CocConfig" to edit coc-settings.json
  * Run ":CocInstall coc-json" to get completion support.
  */
  "$schema": "./data/schema.json",
  "coc.preferences.autoTrigger": "always",
  "coc.preferences.snippets.enable": true,
  // only used when autoTrigger is always
  "coc.preferences.triggerAfterInsertEnter": false,
  "coc.preferences.minTriggerInputLength": 1,
  "coc.preferences.acceptSuggestionOnCommitCharacter": false,
  "coc.preferences.customSources": ["emoji", "include"],
  "coc.preferences.timeout": 2000,
  "coc.preferences.noselect": true,
  "coc.preferences.formatOnType": false,
  "coc.preferences.parseKeywordsLimitLines": 5000,
  "coc.preferences.hyphenAsKeyword": true,
  // "coc.preferences.watchmanPath": "",
  "coc.preferences.jumpCommand": "edit",
  "coc.preferences.diagnostic.locationlist": true,
  "coc.preferences.diagnostic.enable": true,
  "coc.preferences.diagnostic.level": "hint",
  "coc.preferences.diagnostic.enableMessage": true,
  "coc.preferences.diagnostic.highlightOffset": 1000,
  "coc.preferences.diagnostic.signOffset": 1000,
  "coc.preferences.diagnostic.errorSign": ">>",
  "coc.preferences.diagnostic.warningSign": "âš ",
  "coc.preferences.diagnostic.infoSign": ">>",
  "coc.preferences.diagnostic.hintSign": ">>",
  "coc.source.around.enable": true,
  "coc.source.around.shortcut": "A",
  "coc.source.around.priority": 1,
  "coc.source.buffer.enable": true,
  "coc.source.buffer.shortcut": "B",
  "coc.source.buffer.priority": 1,
  "coc.source.buffer.ignoreGitignore": true,
  "coc.source.dictionary.enable": true,
  "coc.source.dictionary.shortcut": "D",
  "coc.source.dictionary.priority": 1,
  "coc.source.emoji.enable": true,
  "coc.source.emoji.shortcut": "EMO",
  "coc.source.emoji.priority": 9,
  "coc.source.file.enable": true,
  "coc.source.file.shortcut": "F",
  "coc.source.file.priority": 10,
  "coc.source.file.triggerCharacters": ["/"],
  "coc.source.file.trimSameExts": [".ts", ".js"],
  "coc.source.file.ignoreHidden": true,
  "coc.source.file.ignorePatterns": [],
  "coc.source.include.enable": true,
  "coc.source.include.shortcut": "I",
  "coc.source.include.priority": 10,
  "coc.source.include.trimSameExts": [".ts", ".js"],
  "coc.source.omni.enable": true,
  "coc.source.omni.shortcut": "O",
  "coc.source.omni.priority": 3,
  "coc.source.omni.filetypes": [],
  "coc.source.tag.enable": true,
  "coc.source.tag.shortcut": "T",
  "coc.source.tag.priority": 3,
  "coc.source.word.enable": true,
  "coc.source.word.shortcut": "10k",
  "coc.source.word.priority": 1,
  "coc.source.word.filetypes": ["markdown"],
  "coc.source.ultisnips.enable": true,
  "coc.source.ultisnips.priority": 99,
  "coc.source.ultisnips.shortcut": "US",
  "coc.source.neosnippet.enable": true,
  "coc.source.neosnippet.priority": 99,
  "coc.source.neosnippet.shortcut": "NS",
  "coc.source.neco.enable": true,
  "coc.source.neco.shortcut": "NECO",
  "coc.source.neco.priority": 9,
  "coc.source.neco.filetypes": ["vim"],
  "coc.source.neco.triggerCharacters": [":", "&"],
  "coc.source.neco.firstMatch": true,
  "coc.source.languageclient.filetypes": [],
  "coc.source.gocode.enable": true,
  "coc.source.gocode.filetypes": ["go"],
  "coc.source.gocode.priority": 99,
  "coc.source.gocode.shortcut": "GOC",
  "coc.source.gocode.triggerCharacters": [".",":"],
  "http.proxy": "",
  "http.proxyStrictSSL": true,
  "languageserver": {}
}
// vim:ft=jsonc
